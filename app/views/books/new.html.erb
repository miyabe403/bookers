<h1>New book</h1>

<!-- form_with部分 オプション追記(厳密な記述) -->
<%= form_with model: @book, url: '/books', method: :post do |f| %>
  <!-- Listモデルのtitle用のフィールドを追加 -->
　<h4>title</h4>
  <%= f.text_field :title %>
  <!-- Listモデルのbody用のフィールドを追加 -->
  <h4>body</h4>
  <%= f.text_area :body %>
  
  <%= f.submit 'Create Book' %>
  
<% end %>